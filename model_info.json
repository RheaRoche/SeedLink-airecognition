            "train": [
                "RandomResizedCrop",
                "RandomHorizontalFlip",
                "RandomVerticalFlip",
                "RandomRotation(30)",
                "ColorJitter",
                "RandomAffine",
                "RandomPerspective",
                "RandomGrayscale",
                "RandomErasing"
            ],
