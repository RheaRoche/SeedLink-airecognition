    "training": {
        "total_epochs": 49,
        "optimizer": "AdamW",
        "learning_rate": "5e-4",
        "scheduler": "OneCycleLR",
        "batch_size": 64,
        "weight_decay": 0.0001,
        "label_smoothing": 0.1,
        "early_stopping_patience": 15
